   "source": [
    "def outter(a,b):\n",
    "  def inner(x):         # Python 函式內部可以定義其他函式\n",
    "      y = a*x + b\n",
    "      return y\n",
    "  return inner          # 傳回值可以是函式\n",
    "\n",
    "f = outter(2,3)         # r 為函式\n",
    "s = f(3)\n",
    "print(s)\n",
    "\n",
    "t = outter(2,3)(3)      # outter(2,3) 為函式\n",
    "print(t)"
   ]
